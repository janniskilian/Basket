<vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <group
        android:name="speechinputclearGroup"
        android:pivotX="12"
        android:pivotY="12">
        <path
            android:name="speechinputPath"
            android:fillAlpha="0"
            android:fillColor="@color/black"
            android:pathData="@string/avd_path_speechinput"/>

        <path
            android:name="clearPath"
            android:fillColor="@color/black"
            android:pathData="@string/avd_path_clear"/>
    </group>
</vector>
