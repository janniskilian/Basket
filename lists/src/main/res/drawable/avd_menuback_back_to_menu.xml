<animated-vector
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:aapt="http://schemas.android.com/aapt"
	android:drawable="@drawable/vd_menuback_back">

	<target android:name="backPath">
		<aapt:attr name="android:animation">
			<objectAnimator
				android:duration="@integer/animation_duration_s"
				android:interpolator="@android:interpolator/fast_out_slow_in"
				android:propertyName="pathData"
				android:valueFrom="@string/avd_path_back"
				android:valueTo="@string/avd_path_menu"
				android:valueType="pathType"/>
		</aapt:attr>
	</target>

	<target android:name="menuBackGroup">
		<aapt:attr name="android:animation">
			<objectAnimator
				android:duration="@integer/animation_duration_s"
				android:interpolator="@android:interpolator/fast_out_slow_in"
				android:propertyName="rotation"
				android:valueFrom="0"
				android:valueTo="-180"/>
		</aapt:attr>
	</target>

</animated-vector>
