<?xml version="1.0" encoding="utf-8"?>
<animated-selector
	xmlns:android="http://schemas.android.com/apk/res/android">

	<item
		android:id="@+id/back"
		android:drawable="@drawable/vd_menuback_back"
		android:state_selected="true"/>

	<item
		android:id="@+id/menu"
		android:drawable="@drawable/vd_menuback_menu"/>

	<transition
		android:drawable="@drawable/avd_menuback_back_to_menu"
		android:fromId="@id/back"
		android:toId="@id/menu"/>

	<transition
		android:drawable="@drawable/avd_menuback_menu_to_back"
		android:fromId="@id/menu"
		android:toId="@id/back"/>
</animated-selector>
