<?xml version="1.0" encoding="utf-8"?>
<animated-selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/checked"
        android:drawable="@drawable/vd_addchecked_checked"
        android:state_selected="true"/>

    <item
        android:id="@+id/add"
        android:drawable="@drawable/vd_addchecked_add"/>

    <transition
        android:drawable="@drawable/avd_addchecked_checked_to_add"
        android:fromId="@id/checked"
        android:toId="@id/add"/>

    <transition
        android:drawable="@drawable/avd_addchecked_add_to_checked"
        android:fromId="@id/add"
        android:toId="@id/checked"/>
</animated-selector>
